{
    "name": "sajtiii/laravel-lockable-attributes",
    "description": "Lock your eloquent model attributes to prevent changes on them.",
    "type": "package",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Attila Tam√°s Nagy",
            "email": "nagyattila949@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0||^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^8.0||^9.0",
        "filament/forms": "^3.2",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Sajtiii\\LockableAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sajtiii\\LockableAttributes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sajtiii\\LockableAttributes\\LockableAttributesServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
